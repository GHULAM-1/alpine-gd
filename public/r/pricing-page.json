      "content": "\"use client\";\n\nimport { DollarSign, ChevronRight } from \"lucide-react\";\n\nexport function PricingSection() {\n  const products = [\n    {\n      id: 0,\n      name: \"Sonic Light\",\n      description: \"Premium Smart Soundbar\",\n      price: \"$599\",\n      originalPrice: \"$699\"\n    },\n    {\n      id: 1,\n      name: \"Sonic Pro\",\n      description: \"Professional Audio System\",\n      price: \"$799\",\n      originalPrice: \"$899\"\n    },\n    {\n      id: 2,\n      name: \"Sonic Max\",\n      description: \"Ultimate Sound Experience\",\n      price: \"$999\",\n      originalPrice: \"$1,199\"\n    }\n  ];\n\n  return (\n    <div className=\"min-h-screen\">\n      <section className=\"pt-16 pb-12 px-4 sm:px-6\">\n        <div className=\"max-w-7xl mx-auto\">\n          <div className=\"flex items-center gap-3 border rounded-full px-4 py-3 w-fit mb-8\">\n            <DollarSign className=\"w-5 h-5\" />\n            <span className=\"text-lg\">Pricing</span>\n          </div>\n\n          <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8 mb-12\">\n            <div>\n              <h1 className=\"text-3xl md:text-4xl lg:text-5xl font-semibold\">\n                Simple Pricing,\n                <br />\n                Exceptional Value\n              </h1>\n            </div>\n            <div>\n              <p className=\"text-lg text-gray-700 dark:text-gray-300 lg:text-right\">\n                We believe great sound shouldn't come with hidden costs or confusing plans.\n              </p>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <section className=\"px-4 sm:px-6 pb-16\">\n        <div className=\"max-w-7xl mx-auto\">\n          <div className=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n            {products.map((product) => (\n              <div key={product.id} className=\"bg-gray-50 dark:bg-gray-800 rounded-2xl p-6\">\n                <div className=\"bg-gray-200 dark:bg-gray-700 rounded-xl aspect-square mb-4 flex items-center justify-center\">\n                  <div className=\"text-6xl\">ðŸ”Š</div>\n                </div>\n\n                <div className=\"flex items-center gap-2 mb-2\">\n                  <span className=\"text-lg font-semibold\">{product.price}</span>\n                  <span className=\"text-gray-500 line-through text-sm\">{product.originalPrice}</span>\n                </div>\n\n                <h3 className=\"text-2xl font-semibold mb-2\">{product.name}</h3>\n                <p className=\"text-gray-600 dark:text-gray-400 mb-4\">{product.description}</p>\n\n                <button className=\"w-full bg-black dark:bg-white text-white dark:text-black py-3 px-6 rounded-full font-medium flex items-center justify-center gap-2\">\n                  Buy now\n                  <ChevronRight className=\"w-4 h-4\" />\n                </button>\n              </div>\n            ))}\n          </div>\n        </div>\n      </section>\n    </div>\n  );\n}",
