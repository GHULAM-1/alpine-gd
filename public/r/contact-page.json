  "files": [
    {
      "path": "components/sections/contact-section.tsx",
      "content": "\"use client\";\n\nimport { Lightbulb, MessageSquare, Phone } from \"lucide-react\";\n\nexport function ContactSection() {\n  return (\n    <section className=\"py-12 sm:py-16 md:py-20 px-4 sm:px-6 md:px-8\">\n      <div className=\"max-w-7xl mx-auto\">\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8 sm:gap-12 md:gap-16\">\n          <div className=\"space-y-6\">\n            <div className=\"space-y-4\">\n              <div className=\"flex items-center gap-3\">\n                <Lightbulb className=\"w-6 h-6 text-[#252525] dark:text-[#FAFAFA]\" />\n                <span className=\"text-lg font-medium text-[#252525] dark:text-[#FAFAFA]\">Contact us</span>\n              </div>\n              <h2 className=\"text-3xl sm:text-4xl md:text-5xl font-semibold text-[#252525] dark:text-[#FAFAFA]\">\n                24/7 Available\n              </h2>\n            </div>\n            <p className=\"text-lg text-[#252525] dark:text-[#FAFAFA]\">\n              Contact us via email or phone. We respond promptly!\n            </p>\n            <div className=\"space-y-4\">\n              <div className=\"flex items-center gap-3\">\n                <MessageSquare className=\"w-5 h-5 text-[#252525] dark:text-[#FAFAFA]\" />\n                <a href=\"mailto:example@gmail.com\" className=\"text-lg text-[#252525] dark:text-[#FAFAFA]\">\n                  example@gmail.com\n                </a>\n              </div>\n              <div className=\"flex items-center gap-3\">\n                <Phone className=\"w-5 h-5 text-[#252525] dark:text-[#FAFAFA]\" />\n                <a href=\"tel:+1-555-44-456\" className=\"text-lg text-[#252525] dark:text-[#FAFAFA]\">\n                  +1-555-44-456\n                </a>\n              </div>\n            </div>\n          </div>\n          <div className=\"bg-gray-100 dark:bg-gray-800 rounded-2xl p-6 sm:p-8\">\n            <h3 className=\"text-2xl font-bold text-[#252525] dark:text-[#FAFAFA] mb-4\">Write us</h3>\n            <form className=\"space-y-4\">\n              <div className=\"grid grid-cols-2 gap-4\">\n                <input type=\"text\" placeholder=\"First name\" className=\"px-4 py-2 rounded-md border\" />\n                <input type=\"text\" placeholder=\"Last name\" className=\"px-4 py-2 rounded-md border\" />\n              </div>\n              <input type=\"email\" placeholder=\"Email\" className=\"w-full px-4 py-2 rounded-md border\" />\n              <textarea placeholder=\"Message\" rows={4} className=\"w-full px-4 py-2 rounded-md border\" />\n              <button type=\"submit\" className=\"w-full bg-[#252525] text-white py-3 rounded-full\">Submit</button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n}",
      "type": "registry:component",
      "target": "components/sections/contact-section.tsx"
    }
  ],
  "tailwind": {
    "config": {
      "theme": {
        "extend": {}
      }
    }
  },
  "meta": {
    "description": "Contact form with contact information section",
    "source": "http://localhost:3000/r/contact-page.json"
  }
